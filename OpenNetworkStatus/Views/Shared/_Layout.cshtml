<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>@ViewBag.Title</title>

    <link rel="stylesheet" media="screen" href="//cdn.jsdelivr.net/npm/bulma@0.8.0/css/bulma.min.css" />
    <link rel="stylesheet" media="screen" href="//cdn.jsdelivr.net/npm/@@creativebulma/bulma-tooltip@1.0.1/dist/bulma-tooltip.min.css" />
    <link rel="stylesheet" media="screen" href="/css/main.css" />
</head>
<body>
    <section class="section">
        <div class="container">
            @RenderBody()
        </div>
    </section>

    <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js" integrity="sha256-4iQZ6BVL4qNKlQ27TExEhBN1HFPvAvAMbFavKKosSWQ=" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/chart.js@2.9.3/dist/Chart.min.js" integrity="sha256-R4pqcOYV8lt7snxMQO/HSbVCFRPMdrhAFMH+vr9giYI=" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/chartjs-plugin-crosshair@1.1.4/dist/chartjs-plugin-crosshair.min.js" integrity="sha256-RXqOU/0mfSr0g02o6n6YoaVohdmh066i82b42w8dUVw=" crossorigin="anonymous"></script>
    <script type="module" src="//unpkg.com/ionicons@5.0.0/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule="" src="//unpkg.com/ionicons@5.0.0/dist/ionicons/ionicons.js"></script>
    
    <script src="/js/main.js"></script>

    @await RenderSectionAsync("Scripts", false)
</body>
</html>